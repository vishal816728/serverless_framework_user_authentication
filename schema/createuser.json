{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "User Schema for creating new user",
    "type": "object",
    "properties": {
        "username":{
            "type":"string",
            "title":"username",
            "minLength":3,
            "maxLength":50
        },
        "password":{
            "type":"string",
            "title":"password",
            "minLength":3,
            "maxLength": 50
        }
    },
    "required":["username","password"]
}